{
    "name": "andrey-helldar/black-or-white-text-color",
    "description": "A simple helper, helping to determine what color the text will look better over a monotonous color.",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/andrey-helldar/black-or-white-text-color",
    "keywords": ["black or white text color", "color", "color of text"],
    "support": {
        "issues": "https://github.com/andrey-helldar/black-or-white-text-color/issues",
        "source": "https://github.com/andrey-helldar/black-or-white-text-color"
    },
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0|^9.0",
        "illuminate/support": "^5.8|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\BlackOrWhiteTextColor\\": "src"
        },
        "files": [
            "src/helpers/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Helldar\\BlackOrWhiteTextColor\\Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Helldar\\BlackOrWhiteTextColor\\ServiceProvider"
            ]
        }
    }
}
